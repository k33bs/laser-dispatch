name = "status-to-discord"
main = "src/index.ts"
compatibility_date = "2024-01-01"
workers_dev = false

# Cron trigger - runs every 10 minutes
[triggers]
crons = ["*/10 * * * *"]

# KV namespace for tracking posted status updates
[[kv_namespaces]]
binding = "EAT_LASERS_STATUS_POSTED_CACHE"
id = "YOUR_KV_NAMESPACE_ID"

# Secrets (set via `wrangler secret put <NAME>`):
# - EAT_LASERS_STATUS_DISCORD_WEBHOOK_URL: Your Discord webhook URL
